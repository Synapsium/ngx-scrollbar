{
    "name": "@synapsium/ngx-scrollbar",
    "version": "1.0.5",
    "description": "Angular wrapper library for beautiful charts based on Chartjs",
    "author": "https://github.com/synapsium/ngx-scrollbar/graphs/contributors",
    "license": "MIT",
    "keywords": [
        "angular",
        "scrollbar",
        "native bar",
        "lightweight"
    ],
    "bugs": {
        "url": "https://github.com/synapsium/ngx-scrollbar/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/Synapsium/ngx-scrollbar.git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-css": "node-sass --include-path scss src/styles/styles.scss dist/@synapsium/ngx-scrollbar/styles/styles.css",
        "copy-scss": "copyfiles -f ./src/styles/styles.scss ./dist/@synapsium/ngx-scrollbar/styles",
        "npmpublish": "npm run build scrollbar && npm run build-css && npm run copy-scss && cd ./dist/@synapsium/ngx-scrollbar && npm publish --access public"
    },
    "private": false,
    "dependencies": {
        "@angular/core": "^15.2.0",
        "resize-observer-polyfill": "^1.5.1"
    },
    "devDependencies": {
        "@angular/common": "^15.2.0",
        "@angular/compiler": "^15.2.0",
        "@angular/platform-browser": "^15.2.0",
        "@angular/platform-browser-dynamic": "^15.2.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.12.0",
        "@angular-devkit/build-angular": "^15.2.9",
        "@angular/cli": "~15.2.9",
        "@angular/compiler-cli": "^15.2.0",
        "@types/jasmine": "~4.3.0",
        "jasmine-core": "~4.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "ng-packagr": "^15.2.2",
        "typescript": "~4.9.4",
        "copyfiles": "^2.1.0"
    }
}
